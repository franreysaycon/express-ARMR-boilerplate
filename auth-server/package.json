{
  "name": "frexpress-auth-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "build": "npm-run-all clean transpile",
    "dev": "npm-run-all build start",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-useragent": "^1.0.13",
    "jsonwebtoken": "^8.5.1",
    "lolex": "^6.0.0",
    "mongodb-memory-server": "^6.5.2",
    "mongoose": "^5.9.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ],
    "legacyWatch": true,
    "pollingInterval": 1000
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true,
      "mocha": true,
      "jest": true
    },
    "parser": "babel-eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2"
  }
}
